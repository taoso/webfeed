{
  "manifest_version": 3,
  "name": "WebFeed",
  "version": "0.8.1",
  "description": "Web Feed Reader",
  "author": "Taoshu",
  "homepage_url": "https://github.com/taoso/webfeed",
  "icons": {
    "48": "icons/icon-square.png"
  },
  "background": "js/background.js",
  "permissions": [
    "alarms",
    "bookmarks",
    "contextMenus",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "128": "icons/icon-square-gray.png"
    },
    "default_title": "WebFeed"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
