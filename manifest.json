{
  "manifest_version": 3,
  "name": "WebFeed",
  "version": "0.8.1",
  "description": "Web Feed Reader",
  "author": "涛叔",
  "developer": {
    "name": "涛叔",
    "url": "https://taoshu.in/"
  },
  "homepage_url": "https://github.com/taoso/webfeed",
  "icons": {
    "48": "icons/icon-square.svg"
  },
  "background": {
    "scripts": ["js/background.js"],
    "type": "module"
  },
  "permissions": [
    "alarms",
    "storage",
    "bookmarks",
    "scripting",
    "activeTab",
    "menus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "page_action": {
    "default_icon": "icons/bar-icon.svg",
    "default_title": "Subscribe feed",
    "pinned": true
  },
  "action": {
    "default_icon": "icons/icon-square.svg",
    "default_title": "WebFeed"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "web-feed@taoshu.in",
      "strict_min_version": "90.0"
    }
  }
}

